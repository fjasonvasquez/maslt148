<div class="inner cover">
  <h1 class="cover-heading"><i>Food News</i></h1>
  
	<div class="row">
		<% @tweets.each do |tweet| %>
			<div class="col-md-6">
				<div class="food-news">
					<div class="card">
						<%= twitter_parser tweet %>
					</div>
				</div>
			</div>
		<% end %>
	</div>
</div>

