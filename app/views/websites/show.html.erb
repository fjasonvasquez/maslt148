<div class="container">
	<div class="row">
		<div class="col-md-7">
			<%= image_tag @website_item.main_image %>
		<div>

		<div class="col-md-5">
			<h1><%= @website_item.title %></h1>

			<hr>

			<em><%= @website_item.subtitle %></em>

			<p><%= @website_item.body %></p>

			<hr>

			<h3>Partners Include:</h3>

			<% @website_item.partners.each do |p| %>
			<p><%= p.name %></p>
			<% end %>
		<div>
	</div>
</div>